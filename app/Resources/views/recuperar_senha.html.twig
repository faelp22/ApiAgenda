{% spaceless %}
    <!DOCTYPE html>
    <html lang="pt-BR">
        <head>
            <meta charset="utf-8"/>
            <meta content="IE=edge" http-equiv="X-UA-Compatible"/>
            <meta content="width=device-width, initial-scale=1" name="viewport"/>
            <meta content="" name="description"/>
            <meta content="Isael Sousa" name="author"/>
            <link href="{{ asset('favicon.ico') }}" rel="icon" type="image/x-icon"/>
            <title>
                {% block title %}Agenda{% endblock %}
            </title>
            {% block stylesheets %}
                <link href="{{ asset('/public/lib/css/all-libs.min.css') }}" rel="stylesheet" />
                <link href="{{ asset('/public/dist/login/login.min.css') }}" rel="stylesheet"/>
            {% endblock %}
        </head>
        <body>
            <div class="container">
                <img class="logo" src="/public/imagens//public/imagens/calendar-512.png" width="50" />
                <form action="{{ path("fos_user_security_check") }}" method="post" class="form-signin">
                    <h3 class="text-center helpdesk">Agenda</h3>
                    <div class="form-group">
                        <input name="_csrf_token" type="hidden" value="{{ csrf_token }}" />
                        <label>Usu√°rio:</label>
                        <input class="form-control" id="email" name="_email" required="" type="email" />
                    </div>
                </form>
            </div> <!-- /container -->
            {% block javascripts %}
                <script src="{{ asset('/public/lib/js/all-libs.min.js') }}"></script>
                <script src="{{ asset('/public/dist/main.min.js') }}"></script>
            {% endblock %}
        </body>
    </html>
{% endspaceless %}